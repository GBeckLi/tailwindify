{
  "name": "tailwindify",
  "version": "1.0.1",
  "main": "./dist/bin/index.js",
  "license": "MIT",
  "bin": {
    "twify": "./dist/bin/index.js"
  },
  "scripts": {
    "build:prod": "./node_modules/.bin/ts-node ./bin/build.ts",
    "build": "./node_modules/.bin/webpack --config webpack.config.js",
    "watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' ./index.ts",
    "start": "./node_modules/.bin/ts-node ./bin/index.ts",
    "tsc": "tsc -p .",
    "refresh": "rm -rf ./node_modules ./package-lock.json ./yarn.lock && yarn install"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/clear": "^0.1.0",
    "@types/commander": "^2.12.2",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.12.21",
    "@types/shelljs": "^0.8.6",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "file-loader": "^5.0.2",
    "nodemon": "^2.0.2",
    "shebang-loader": "^0.0.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "typescript-cli": "^0.1.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "autoprefixer": "^9.7.3",
    "chalk": "^3.0.0",
    "clear": "^0.1.0",
    "commander": "^4.0.1",
    "figlet": "^1.2.4",
    "inquirer": "^7.0.1",
    "shelljs": "^0.8.3"
  }
}
